{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "(gdb) Launch",
        "type": "lldb",
        "request": "launch",
        "program": "${workspaceFolder}/target/debug/zeptwo",
        "args": ["src/main.zp"],
        "cwd": "${workspaceFolder}",
        "stopOnEntry": false,
        "sourceLanguages": ["rust"],
        "preLaunchTask": "cargo build"
      }
    ]
  }
  


